<?xml version="1.0" encoding="utf-8"?> 
<configuration>
  <packageRestore>
    <!-- enabled/False opts out of both Automatic Package Restore and MSBuild-Integrated Package Restore -->
    <add key="enabled" value="True" />

    <!-- automatic/False Opts out of Automatic Package Restore in Visual Studio -->
    <add key="automatic" value="True" />
  </packageRestore>
</configuration>

